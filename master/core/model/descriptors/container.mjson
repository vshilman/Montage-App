{
  "blueprint_container_config": {
    "prototype": "montage/core/meta/property-blueprint",
    "properties": {
      "name": "config",
      "blueprint": {"@": "root"},
      "valueType": "object"
    }
  },
  "blueprint_container_description": {
    "prototype": "montage/core/meta/property-blueprint",
    "properties": {
      "name": "description",
      "blueprint": {"@": "root"},
      "valueType": "String"
    }
  },
  "blueprint_container_devices": {
    "prototype": "montage/core/meta/property-blueprint",
    "properties": {
      "name": "devices",
      "blueprint": {"@": "root"},
      "valueType": "array"
    }
  },
  "blueprint_container_enabled": {
    "prototype": "montage/core/meta/property-blueprint",
    "properties": {
      "name": "enabled",
      "blueprint": {"@": "root"},
      "valueType": "boolean"
    }
  },
  "blueprint_container_id": {
    "prototype": "montage/core/meta/property-blueprint",
    "properties": {
      "name": "id",
      "blueprint": {"@": "root"},
      "valueType": "String"
    }
  },
  "blueprint_container_immutable": {
    "prototype": "montage/core/meta/property-blueprint",
    "properties": {
      "name": "immutable",
      "blueprint": {"@": "root"},
      "valueType": "boolean"
    }
  },
  "blueprint_container_name": {
    "prototype": "montage/core/meta/property-blueprint",
    "properties": {
      "name": "name",
      "blueprint": {"@": "root"},
      "valueType": "String"
    }
  },
  "blueprint_container_target": {
    "prototype": "montage/core/meta/property-blueprint",
    "properties": {
      "name": "target",
      "blueprint": {"@": "root"},
      "valueType": "String"
    }
  },
  "blueprint_container_template": {
    "prototype": "montage/core/meta/property-blueprint",
    "properties": {
      "name": "template",
      "blueprint": {"@": "root"},
      "valueType": "object"
    }
  },
  "blueprint_container_type": {
    "prototype": "montage/core/meta/property-blueprint",
    "properties": {
      "name": "type",
      "blueprint": {"@": "root"},
      "valueType": "String",
      "enumValues": [
        "JAIL",
        "VM",
        "DOCKER"
      ]
    }
  },
  "root": {
    "prototype": "core/model/model-descriptor",
    "properties": {
      "name": "Container",
      "propertyBlueprints": [
        {"@": "blueprint_container_config"},
        {"@": "blueprint_container_description"},
        {"@": "blueprint_container_devices"},
        {"@": "blueprint_container_enabled"},
        {"@": "blueprint_container_id"},
        {"@": "blueprint_container_immutable"},
        {"@": "blueprint_container_name"},
        {"@": "blueprint_container_target"},
        {"@": "blueprint_container_template"},
        {"@": "blueprint_container_type"}
      ]
    }
  }
}